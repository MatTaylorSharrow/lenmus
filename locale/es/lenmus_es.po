#, c-format
msgid "Element '%s' has less parameters than the minimum required. Element ignored."
msgstr "Elemento '%s' tiene menos parámetros de los necesarios. Se ignora el elemento."

#: ldp_parser/LDPParser.cpp:1182
#, c-format
msgid "Element '%s' has an incoherent value: go forward to start?. Element ignored"
msgstr "Elemento '%s' tiene un valor incoherente: ¿ir adelante al principio?. Se ignora el elemento."

#: ldp_parser/LDPParser.cpp:1191
#, c-format
msgid "Element '%s' has an incoherent value: go backwards to end?. Element ignored"
msgstr "Elemento '%s' tiene un valor incoherente: ¿ir atras al final?. Se ignora el elemento."

