#, c-format
msgid "Time shift: Letter %s is not a valid note duration. Replaced by a quarter note"
msgstr "Desplazamiento temporal: letra %s no es una duración válida. Se sustituye por una negra."

#: ldp_parser/LDPParser.cpp:1400
#, c-format
msgid "Missing parameters in rest '%s'. Replaced by '(%s %s)'."
msgstr "Faltan parámetros en silencio '%s'. SE sustituye por '(%s %s)'."

#: ldp_parser/LDPParser.cpp:1408
#, c-format
msgid "Missing parameters in note '%s'. Assumed (%s c4 %s)."
msgstr "Faltan parámetros en nota '%s'. Se utilizará (%s c4 %s)."

#: ldp_parser/LDPParser.cpp:1443
#, c-format
msgid "Unknown note pitch '%s'. Assumed 'c4'."
msgstr "Altura de nota desconocida en '%s'. Se usará 'c4'."

#: ldp_parser/LDPParser.cpp:1450
#: ldp_parser/LDPParser.cpp:2262
#: ldp_parser/LDPParser.cpp:2285
#, c-format
msgid "Unknown note/rest duration '%s'. A quarter note assumed."
msgstr "Tipo de nota desconocido en '%s'. Se supone negra."

#: ldp_parser/LDPParser.cpp:1485
msgid "Requesting beaming a note longer than eight. Beaming ignored."
msgstr "Sa solicita agrupar una nota mayor que la corchea. Agrupación ignorada. "

