#: ldp_parser/LDPParser.cpp:375
#, c-format
msgid "Element RAIZ expected but found element %s. Analysis stopped."
msgstr "Se esperaba elemento 'RAIZ' pero viene un elemento '%s'. Análisis cancelado."

#: ldp_parser/LDPParser.cpp:508
msgid "Syntax error: more closing parenthesis than parenthesis opened. Analysis stopped."
msgstr "Error de sintaxis: hay más paréntesis de cierre que de apertura. Análisis cancelado."

#: ldp_parser/LDPParser.cpp:533
#, c-format
msgid "Element 'score' expected but found element %s. Analysis stopped."
msgstr "Se esperaba elemento 'Partitura' pero viene un elemento '%s'. Análisis cancelado."

#: ldp_parser/LDPParser.cpp:547
#, c-format
msgid "Element 'vers' expected but found element %s. Analysis stopped."
msgstr "Se esperaba elemento 'Vers' pero viene un elemento '%s'. Análisis cancelado."

#: ldp_parser/LDPParser.cpp:574
#, c-format
msgid "Error analysing LDP score: LDP version (%d) not supported. Analysis stopped."
msgstr "Error analizando una partitura LDP: La versión de LDP (%d) no está soportada. Análisis cancelado."

#: ldp_parser/LDPParser.cpp:608
#, c-format
msgid "Element 'NumInstrumentos' expected but found element %s. Analysis stopped."
msgstr "Se esperaba elemento 'NumInstrumentos' pero viene un elemento '%s'. Análisis cancelado."

#: ldp_parser/LDPParser.cpp:707
#: ldp_parser/LDPParser.cpp:965
#, c-format
msgid "Element '%s' expected but found element %s. Analysis stopped."
msgstr "Se esperaba elemento '%s' pero viene un elemento '%s'. Análisis cancelado."

#: ldp_parser/LDPParser.cpp:765
#, c-format
msgid "Number of staves expected but found '%s'. Element '%s' ignored."
msgstr "Se esperaba el número de pentagramas pero viene '%s'. Se ignora el elemento '%s' ."

#: ldp_parser/LDPParser.cpp:771
#, c-format
msgid "Expected value for %s but found element '%s'. Ignored."
msgstr "Se esperaba un valor para '%s' pero viene un elemento '%s'. Se ignora"

#: ldp_parser/LDPParser.cpp:776
#, c-format
msgid "[%s]: unknown element '%s' found. Element ignored."
msgstr "[%s]: encontrado elemento desconocido '%s'. Se ignora."

#: ldp_parser/LDPParser.cpp:786
#, c-format
msgid "Expected '%s' but found element %s. Analysis stopped."
msgstr "Se esperaba elemento '%s' pero viene un elemento '%s'. Análisis cancelado."

#: ldp_parser/LDPParser.cpp:815
#, c-format
msgid "Expected '%s' but found element %s. Element ignored."
msgstr "Se esperaba '%s' pero viene un elemento '%s'. Se ignora el elemento."

#: ldp_parser/LDPParser.cpp:905
#: ldp_parser/LDPParser.cpp:2232
#, c-format
msgid "[AnalyzeMusicData]: Unknown or not allowed element '%s' found. Element ignored."
msgstr "[AnalyzeMusicData]: Elemento '%s' desconocido o no permitido en este punto. Se ignora."

#: ldp_parser/LDPParser.cpp:936
#, c-format
msgid "[AnalyzeChord]: Expecting notes found element '%s'. Element ignored."
msgstr "[AnalyzeChord]: Se esperaba un elemento Nota pero viene '%s'. Elemento ignorado."

#: ldp_parser/LDPParser.cpp:985
#, c-format
msgid "Element %s expected but found element %s. Analysis stopped."
msgstr "Se esperaba elemento '%s' pero viene un elemento '%s'. Análisis cancelado."

#: ldp_parser/LDPParser.cpp:1037
#, c-format
msgid "Expected node '%s' but found node '%s'. Analysis ended."
msgstr "Se esperaba un nodo '%s' pero viene un nodo '%s'. Análisis cancelado."

#: ldp_parser/LDPParser.cpp:1056
#, c-format
msgid "Expected 'numero de parte' but found '%s'. Analysis stopped."
msgstr "Se esperaba un 'número de parte' pero viene '%s'. Análisis cancelado."

#: ldp_parser/LDPParser.cpp:1068
#, c-format
msgid "Number of staves expected but found '%s'. Analysis stopped."
msgstr "Se esperaba el número de pentagramas pero viene '%s'. Analisis cancelado."

#: ldp_parser/LDPParser.cpp:1149
#, c-format
msgid "[AnalyzeMeasure]: Expected node 'Figura', 'Grupo', 'Atributo' or 'Desplazamiento' but found node '%s'. Node ignored."
msgstr "[AnalyzeMeasure]: Se esperaba nodo 'Figura', 'Grupo', 'Atributo' o 'Desplazamiento' pero vieneun nodo '%s'. Nodo ignorado."

#: ldp_parser/LDPParser.cpp:1182
#: ldp_parser/LDPParser.cpp:1872
#: ldp_parser/LDPParser.cpp:2497
#: ldp_parser/LDPParser.cpp:2579
#: ldp_parser/LDPParser.cpp:2650
#: ldp_parser/LDPParser.cpp:2807
#: ldp_parser/LDPParser.cpp:2825
#, c-format
msgid "Element '%s' has less parameters than the minimum required. Element ignored."
msgstr "Elemento '%s' tiene menos parámetros de los necesarios. Se ignora el elemento."

#: ldp_parser/LDPParser.cpp:1194
#, c-format
msgid "Element '%s' has an incoherent value: go forward to start?. Element ignored"
msgstr "Elemento '%s' tiene un valor incoherente: ¿ir adelante al principio?. Se ignora el elemento."

#: ldp_parser/LDPParser.cpp:1203
#, c-format
msgid "Element '%s' has an incoherent value: go backwards to end?. Element ignored"
msgstr "Elemento '%s' tiene un valor incoherente: ¿ir atras al final?. Se ignora el elemento."

#: ldp_parser/LDPParser.cpp:1263
#, c-format
msgid "Time shift: Letter %s is not a valid note duration. Replaced by a quarter note"
msgstr "Desplazamiento temporal: letra %s no es una duración válida. Se sustituye por una negra."

#: ldp_parser/LDPParser.cpp:1413
#, c-format
msgid "Missing parameters in rest '%s'. Replaced by '(%s %s)'."
msgstr "Faltan parámetros en silencio '%s'. SE sustituye por '(%s %s)'."

#: ldp_parser/LDPParser.cpp:1421
#, c-format
msgid "Missing parameters in note '%s'. Assumed (%s c4 %s)."
msgstr "Faltan parámetros en nota '%s'. Se utilizará (%s c4 %s)."

#: ldp_parser/LDPParser.cpp:1459
#, c-format
msgid "Unknown note pitch '%s'. Assumed 'c4'."
msgstr "Altura de nota desconocida en '%s'. Se usará 'c4'."

#: ldp_parser/LDPParser.cpp:1467
#: ldp_parser/LDPParser.cpp:2282
#: ldp_parser/LDPParser.cpp:2305
#, c-format
msgid "Unknown note/rest duration '%s'. A quarter note assumed."
msgstr "Tipo de nota desconocido en '%s'. Se supone negra."

#: ldp_parser/LDPParser.cpp:1502
msgid "Requesting beaming a note longer than eight. Beaming ignored."
msgstr "Sa solicita agrupar una nota mayor que la corchea. Agrupación ignorada. "

#: ldp_parser/LDPParser.cpp:1509
msgid "Requesting to start a beamed group but there is already an open group. Beaming ignored."
msgstr "(%s) Se solicita iniciar una tupla pero ya hay una abierta. Se ignora."

#: ldp_parser/LDPParser.cpp:1530
msgid "Request to end beaming a group in a note that is note the first one of a chord. Beaming ignored."
msgstr "Se solicita finalizar grupo en una nota que no es la primera del acorde. Se ignora."

#: ldp_parser/LDPParser.cpp:1537
msgid "Request to end beaming a group but there is not a  previous note. Beaming ignored."
msgstr "Se solicita finalizar grupo pero no hay ninguno iniciado. Se ignora."

#: ldp_parser/LDPParser.cpp:1545
