#: ldp_parser/LDPParser.cpp:2650
#: ldp_parser/LDPParser.cpp:2807
#: ldp_parser/LDPParser.cpp:2825
#, c-format
msgid "Element '%s' has less parameters than the minimum required. Element ignored."
msgstr "L'élément '%s' a moins de paramètres que le minimum requis. Elément ignoré."

#: ldp_parser/LDPParser.cpp:1194
#, c-format
msgid "Element '%s' has an incoherent value: go forward to start?. Element ignored"
msgstr "L'élément '%s' a une valeure incohérente : poursuivez pour commencer ? Elément ignoré."

#: ldp_parser/LDPParser.cpp:1203
#, c-format
msgid "Element '%s' has an incoherent value: go backwards to end?. Element ignored"
msgstr "L'élément '%s' a une valeure incohérente : reculez pour terminer ? Elément ignoré."

#: ldp_parser/LDPParser.cpp:1263
#, c-format
msgid "Time shift: Letter %s is not a valid note duration. Replaced by a quarter note"
msgstr "Changement de temps : lettre %s n'est pas une durée de note valide. Remplacez par une noire."

#: ldp_parser/LDPParser.cpp:1413
#, c-format
msgid "Missing parameters in rest '%s'. Replaced by '(%s %s)'."
msgstr "Paramètres manquants dans silence '%s'. Remplacé par '(%s %s)'."

#: ldp_parser/LDPParser.cpp:1421
#, c-format
msgid "Missing parameters in note '%s'. Assumed (%s c4 %s)."
msgstr "Paramètres manquants dans la note '%s'. (%s do4 %s) supposé."

#: ldp_parser/LDPParser.cpp:1459
#, c-format
msgid "Unknown note pitch '%s'. Assumed 'c4'."
msgstr "Hauteur de note inconnue '%s'. 'do4' supposé."

#: ldp_parser/LDPParser.cpp:1467
#: ldp_parser/LDPParser.cpp:2282
#: ldp_parser/LDPParser.cpp:2305
#, c-format
msgid "Unknown note/rest duration '%s'. A quarter note assumed."
msgstr "Durée de note/silence inconnu '%s'. Noire supposée."

#: ldp_parser/LDPParser.cpp:1502
msgid "Requesting beaming a note longer than eight. Beaming ignored."
msgstr "Tentative de groupement d'une note plus longue qu'une croche. Groupement ignoré."

#: ldp_parser/LDPParser.cpp:1509
msgid "Requesting to start a beamed group but there is already an open group. Beaming ignored."
msgstr "Nouveau groupement demandé alors qu'un groupement est déjà ouvert. Groupement ignoré."

#: ldp_parser/LDPParser.cpp:1530
msgid "Request to end beaming a group in a note that is note the first one of a chord. Beaming ignored."
msgstr "Fermeture d'un groupement demandé pour une note qui appartient à un accord. Groupement ignoré."

#: ldp_parser/LDPParser.cpp:1537
msgid "Request to end beaming a group but there is not a  previous note. Beaming ignored."
